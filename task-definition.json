[
  {
    "name": "${container_name}",
    "image": "${image_url}",
    "cpu": 256,
    "memory": 512,
    "entryPoint": [],
    "environment": [{ "name" : "ENDPOINT", "value" : "${alb}" }],
    "portMappings": [
      {
        "hostPort": 0,
        "containerPort": ${containerPort},
        "protocol": "tcp"
      }
    ],
    "volumesFrom": [],
    "links": [],
    "mountPoints": [
      {
        "sourceVolume": "efs",
        "containerPath": "/mnt",
        "readOnly": false
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    },
    "essential": true
  }
]
